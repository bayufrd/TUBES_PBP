<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF"
        tools:context=".SapiSaya_Panel">

    <ImageView
            android:id="@+id/imageButtonBatal"
            android:layout_width="110dp"
            android:layout_height="38dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/batal" />

    <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="98dp"
            android:layout_marginTop="22dp"
            android:layout_marginEnd="80dp"
            android:layout_marginBottom="1dp"
            android:text="Sentuh list untuk masuk ke menu edit"
            android:textColor="#000000"
            app:layout_constraintBottom_toTopOf="@+id/RecyclerTemp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageButtonBatal" />

    <ImageView
            android:id="@+id/imageButtonSapisaya2"
            android:layout_width="67dp"
            android:layout_height="53dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="316dp"
            android:onClick="buttonGambar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/sapisaya" />

    <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/RecyclerTemp"
            android:layout_width="416dp"
            android:layout_height="646dp"
            android:layout_marginTop="1dp"
            android:background="#FFFFFF"
            android:scrollbars="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3" />
</androidx.constraintlayout.widget.ConstraintLayout>